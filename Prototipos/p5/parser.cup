//===== Start production =====

expr ::= profile | echo | var;
profile ::= PROFILE id LBRACE (commands | bind | var)* RBRACE

id ::= ID
bind ::= BIND (SKEY | KEY | var)? SEMICOLON
echo ::= ECHO SEMICOLON
var ::= ALIAS ID LBRACE action RBRACE

action ::= (PLUS_JUMP SEMICOLON
            | MINUS_JUMP SEMICOLON
