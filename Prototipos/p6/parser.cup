non terminal expr, profile, echo, var, id, commands, bind, action;

//===== Start production =====

expr ::= profile

profile ::= PROFILE id LBRACE (commands | bind | var | echo | var)* RBRACE ;

id ::= ID ;

bind ::= BIND (SKEY | id )? (action | var)? SEMICOLON ;

echo ::= ECHO SEMICOLON ;

var ::= ALIAS ID LBRACE action RBRACE ;

action ::=    PLUS_JUMP 
            | MINUS_JUMP 
            | PLUS_DUCK 
            | MINUS_DUCK 
            | PLUS_ATTACK 
            | MINUS_ATTACK 
            | PLUS_RELOAD 
            | MINUS_RELOAD 
            | PLUS_USE 
            | MINUS_USE 
            | PLUS_SCORE 
            | MINUS_SCORE ;

ns ::= NUMBER SEMICOLON

